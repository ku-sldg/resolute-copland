	publisher = {Association for Computing Machinery},
	series = {STC '08},
	title = {Remote Attestation on Program Execution},
	url = {https://doi.org/10.1145/1456455.1456458},
	year = {2008},
	bdsk-url-1 = {https://doi.org/10.1145/1456455.1456458}}

