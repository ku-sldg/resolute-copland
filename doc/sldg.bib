	location = {Miami, Florida, USA},
	numpages = {8},
	pages = {9--16},
	publisher = {Association for Computing Machinery},
	series = {PM2HW2N '11},
	title = {A TPM-Enabled Remote Attestation Protocol (TRAP) in Wireless Sensor Networks},
	url = {https://doi.org/10.1145/2069087.2069090},
